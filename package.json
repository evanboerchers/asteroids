{
  "name": "asteroids",
  "version": "1.0.0",
  "description": "classic asteroids game build in godot",
  "scripts": {
    "prebuild": "rimraf dist && mkdirp dist",
    "build": "godot --headless --path . --export-release \"Web\"",
    "serve": "http-server dist",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanboerchers/asteroids.git"
  },
  "author": "Evan Boerchers",
  "license": "ISC",
  "dependencies": {
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.0.1"
  }
}
